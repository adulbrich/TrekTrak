{
  "expo": {
    "name": "TrekTrak",
    "slug": "trektrak",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/osu_badge.png",
    "scheme": "osu.fsf",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/osu_badge.png",
      "resizeMode": "contain",
      "imageResizeMode": "contain",
      "backgroundColor": "#3F6122"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.TrekTrak",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.TrekTrak",
      "version": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/osu_badge.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6e08a6bc-788a-443f-8298-12743306d9fd"
      }
    }
  }
}